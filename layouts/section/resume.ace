= content main
  {{range first 1 .Data.Pages}}
    {{.Render "resume"}}
  {{end}}

= content footer
  .row
    .col-md-12
      p.
        Last Updated:
          {{if .Date.IsZero}}
            {{dateFormat "2 Jan 2006" .Site.LastChange}}
              {{else}}
                {{dateFormat "2 Jan 2006" .Date}}
              {{end}}
